{
    "name": "laravolt/password",
    "description": "Laravel password extended",
    "keywords": [
        "laravel",
        "laravolt",
        "password"
    ],
    "homepage": "https://github.com/laravolt/password",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/auth": "5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.6.*|5.7.*|5.8.*",
        "illuminate/database": "5.6.*|5.7.*|5.8.*",
        "nesbot/carbon": "^1.26.3 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Password\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Password\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Password\\ServiceProvider"
            ],
            "aliases": {
                "Password": "Laravolt\\Password\\Facade"
            }
        }
    }
}
